{
    "name": "Linin",
    "description": "CHrome Extension",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/img/link16.png",
        "32": "/img/link32.png",
        "128": "/img/link128.png"
      }
    },
    "icons": {
        "16": "/img/link16.png",
        "32": "/img/link32.png",
        "128": "/img/link128.png"
    },
    "options_page": "options.html"
}